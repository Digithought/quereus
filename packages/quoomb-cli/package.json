{
  "name": "@quereus/quoomb-cli",
  "version": "0.2.1",
  "type": "module",
  "description": "Quoomb CLI - Interactive REPL for Quereus SQL engine",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "quoomb": "./dist/bin/quoomb.js"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "tsc && node dist/bin/quoomb.js",
    "lint": "eslint src/**/*.ts",
    "test": "vitest",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.28.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@quereus/plugin-loader": "workspace:^",
    "@quereus/quereus": "workspace:^",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "papaparse": "^5.5.3",
    "readline": "^1.3.0"
  }
}
