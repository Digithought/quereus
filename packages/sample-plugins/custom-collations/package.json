{
  "name": "@quereus/quereus-plugin-custom-collations",
  "version": "1.0.0",
  "type": "module",
  "description": "Custom collation functions for specialized sorting behaviors",
  "author": "Quereus Team",
  "keywords": ["quereus-plugin", "quereus", "collations", "sorting"],
  "exports": {
    "./plugin": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@quereus/quereus": "*"
  },
  "engines": {
    "quereus": "^0.3.0"
  },
  "quereus": {
    "provides": { "collations": ["NUMERIC", "LENGTH", "REVERSE", "ALPHANUM", "PHONETIC"] }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@quereus/quereus": "*",
    "typescript": "^5.0.0"
  }
}
